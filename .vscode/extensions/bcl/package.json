{
    "name": "bcl",
    "displayName": "bcl",
    "description": "Block Configuration Language",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.98.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [ {
                "id": "bcl",
                "aliases": [ "bcl", "bcl" ],
